#include <stdio.h>

int main() {
    int idade;
    int contador_maiores = 0;

    printf("Digite a idade de 5 pessoas:\n");

    for (int i = 0; i < 5; i++) {
        printf("Pessoa %d: ", i + 1);
        scanf("%d", &idade);

        if (idade > 18) {
            contador_maiores++;
        }
    }

    printf("NÃºmero de pessoas maiores que 18 anos: %d\n", contador_maiores);

    return 0;
}
